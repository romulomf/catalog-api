@using CatalogApiClient.Models

@model IEnumerable<CategoryViewModel>

<table>
	<thead>
		<tr>
			<td scope="col">nome</td>
			<td scope="col">imagem</td>
		</tr>
	</thead>
	<tbody>
		@foreach(var category in Model)
		{
			<tr>
				<td>@category.Name</td>
				<td>@category.ImageUrl</td>
			</tr>
		}
	</tbody>
</table>